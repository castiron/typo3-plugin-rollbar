{
  "name": "castiron/typo3-plugin-rollbar",
  "type": "typo3-cms-extension",
  "description": "A Rollbar integration for TYPO3 (https://rollbar.com)",
  "homepage": "https://github.com/castiron/typo3-plugin-rollbar",
  "license": ["GPL-2.0+"],
  "keywords": ["TYPO3 CMS"],
  "typo3/cms-core": ">=6.2.0,<8.0",
  "autoload": {
    "psr-4": {
      "CIC\\Rollbar\\": "Classes"
    }
  },
  "repositories": [
    { "type": "vcs", "url": "git@github.com:castiron/cicbase.git" }  
  ],
  "require": {
    "rollbar/rollbar": "^1.1",
    "castiron/typo3-plugin-cicbase": "dev-master"
  }
}
